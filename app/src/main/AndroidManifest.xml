<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hh.androidbaselibrary">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyAppCompat"
        tools:replace="android:icon,android:label,android:theme">
        <activity
            android:name=".ui.dialog.AlertDialogActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginView.LoginViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.clickItem.ClickItemViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.titleView.TitleViewActivity"
            android:exported="false" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>