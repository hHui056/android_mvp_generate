<?xml version="1.0" encoding="utf-8" standalone="no"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.hh.androidbaselibrary">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyAppCompat"
        tools:replace="android:name,android:icon,android:label,android:theme,android:roundIcon">
        <activity
            android:name=".ui.recycler.SimpleNormalRecyclerViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.tab.TabLayoutActivity"
            android:exported="false" />
        <activity
            android:name=".ui.registerView.RegisterViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.baseView.BaseViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.loginView.LoginViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.clickItem.ClickItemViewActivity"
            android:exported="false" />
        <activity
            android:name=".ui.titleView.TitleViewActivity"
            android:exported="false" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hh.androidbaselibrary.ui.dataStore.view.DataStoreActivity"
            android:exported="false" />
        <activity
            android:name="com.hh.androidbaselibrary.ui.objectBox.view.ObjectBoxActivity"
            android:exported="false" />
    </application>

</manifest>